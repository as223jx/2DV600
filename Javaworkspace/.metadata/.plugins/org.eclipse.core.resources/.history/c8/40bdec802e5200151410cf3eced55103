package exercise2;

import java.util.Scanner;

public class LargestK {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		System.out.print("Type an integer: ");
		int n = scan.nextInt();
		
		boolean b = true;
		int count = 0;
		int k = 0;
		
		for (int i = 0; count < n; i+=2) {
			count += i;
			k = i -=2;
		}
		

		
		System.out.print("LargestK: " + k);
		
	}
	
}